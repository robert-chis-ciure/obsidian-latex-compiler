{
  "name": "obsidian-latex-compiler",
  "version": "0.1.0",
  "description": "Obsidian plugin for LaTeX project compilation",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "obsidian",
    "latex",
    "compiler",
    "pdf"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "obsidian": "latest",
    "ts-jest": "^29.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
